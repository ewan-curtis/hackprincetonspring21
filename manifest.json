{
    "manifest_version": 2,
    "name": "Chrome Plugin",
    "description": "Testing Plugin",
    "version": "1.0",
    "content_scripts": [{
        "matches": [
            "http://rooms.tigerapps.org/table"
        ],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}