{
    "manifest_version": 2,
    "name": "roomExport",
    "description": "Easily export room data from rooms.tigerapps.org",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "*://rooms.tigerapps.org/table"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://rooms.tigerapps.org/table"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}